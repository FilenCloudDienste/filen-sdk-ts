{
	"name": "@filen/filen-sdk",
	"version": "1.0.0",
	"description": "Filen SDK",
	"main": "dist/bundle.node.js",
	"browser": "dist/bundle.browser.js",
	"types": "dist/types/index.d.ts",
	"private": false,
	"scripts": {
		"test": "jest --forceExit ./__tests__",
		"dev": "ts-node src/test/index.ts",
		"build": "npm run lint && tsc --emitDeclarationOnly && webpack --config webpack.config.js && npm run typedoc",
		"lint": "eslint src/**/* --ext .js,.jsx,.ts,.tsx",
		"typedoc": "typedoc --plugin typedoc-plugin-missing-exports --out docs src/index.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FilenCloudDienste/filen-sdk-ts.git"
	},
	"keywords": [
		"filen"
	],
	"author": "Filen",
	"license": "AGPLv3",
	"bugs": {
		"url": "https://github.com/FilenCloudDienste/filen-sdk-ts/issues"
	},
	"homepage": "https://filen.io",
	"devDependencies": {
		"@babel/core": "^7.23.9",
		"@babel/preset-env": "^7.23.9",
		"@jest/globals": "^29.7.0",
		"@types/crypto-js": "^4.2.2",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"assert": "^2.1.0",
		"babel-loader": "^9.1.3",
		"browserify-zlib": "^0.2.0",
		"buffer": "^6.0.3",
		"crypto-browserify": "^3.12.0",
		"eslint": "^8.56.0",
		"http-browserify": "^1.7.0",
		"https-browserify": "^1.0.0",
		"jest": "^29.7.0",
		"os-browserify": "^0.3.0",
		"path-browserify": "^1.0.1",
		"stream-browserify": "^3.0.0",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"typedoc": "^0.25.7",
		"typedoc-plugin-missing-exports": "^2.2.0",
		"typescript": "^5.3.3",
		"util": "^0.12.5",
		"webpack": "^5.90.0",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"axios": "^1.6.7",
		"crypto-js": "^4.2.0"
	}
