{
	"name": "@filen/filen-sdk",
	"version": "1.0.3",
	"description": "Filen SDK",
	"main": "dist/bundle.node.js",
	"browser": "dist/bundle.browser.js",
	"types": "dist/types/index.d.ts",
	"private": false,
	"scripts": {
		"test": "jest --forceExit ./__tests__",
		"dev": "ts-node --files dev/index.ts",
		"build": "npm run clear && npm run lint && npm run emitTypes && npm run typedoc && npm run bundle",
		"lint": "eslint src/**/* --ext .js,.jsx,.ts,.tsx",
		"emitTypes": "tsc --emitDeclarationOnly",
		"bundle": "webpack --mode production --node-env production --config webpack.config.js",
		"typedoc": "typedoc --plugin typedoc-plugin-missing-exports --out docs src/index.ts",
		"clear": "npm run rmdir -- dist docs",
		"rmdir": "node -e \"var fs = require('fs'); try{process.argv.slice(1).map((fpath) => fs.rmdirSync(fpath, { recursive: true }))}catch(err){console.log(`Dir ${fpath} not found`)}; process.exit(0);\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FilenCloudDienste/filen-sdk-ts.git"
	},
	"keywords": [
		"filen"
	],
	"author": "Filen",
	"license": "AGPLv3",
	"bugs": {
		"url": "https://github.com/FilenCloudDienste/filen-sdk-ts/issues"
	},
	"homepage": "https://filen.io",
	"devDependencies": {
		"@babel/core": "^7.23.9",
		"@babel/preset-env": "^7.23.9",
		"@babel/preset-typescript": "^7.23.3",
		"@jest/globals": "^29.7.0",
		"@types/crypto-js": "^4.2.2",
		"@types/fs-extra": "^11.0.4",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"assert": "^2.1.0",
		"babel-loader": "^9.1.3",
		"babel-plugin-transform-remove-console": "^6.9.4",
		"browserfs": "^1.4.3",
		"browserify-zlib": "^0.2.0",
		"buffer": "^6.0.3",
		"concurrently": "^8.2.2",
		"crypto-browserify": "^3.12.0",
		"eslint": "^8.56.0",
		"http-browserify": "^1.7.0",
		"https-browserify": "^1.0.0",
		"jest": "^29.7.0",
		"node-polyfill-webpack-plugin": "^3.0.0",
		"os-browserify": "^0.3.0",
		"path-browserify": "^1.0.1",
		"stream-browserify": "^3.0.0",
		"terser-webpack-plugin": "^5.3.10",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"typedoc": "^0.25.7",
		"typedoc-plugin-missing-exports": "^2.2.0",
		"typescript": "^5.3.3",
		"util": "^0.12.5",
		"webpack": "^5.90.0",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"axios": "^1.6.7",
		"crypto-api-v1": "^1.0.0",
		"crypto-js": "^4.2.0",
		"fs-extra": "^11.2.0",
		"js-crypto-key-utils": "^1.0.7",
		"uuid": "^9.0.1"
	}
}
